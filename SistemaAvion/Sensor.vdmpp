class Sensor
types
    public Parametro = <AoB> | <Flaps> | <Velocidad>;
instance variables
    private mediciones: map Parametro to real;
operations
    public Sensor: map Parametro to real ==> Sensor
    Sensor(med) == (
        mediciones := med;
    );

    public getMediciones: () ==> map Parametro to real
    getMediciones() == (
        return mediciones;
    );
end Sensor